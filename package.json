{
  "name": "vscode-python-timeit",
  "displayName": "Python TimeIt",
  "description": "View the execution time (as a comment) of any Python function using custom argument values.",
  "version": "1.3.1",
  "publisher": "HasnainRoopawalla",
  "repository": {
    "type": "git",
    "url": "https://github.com/hasnainroopawalla/vscode-python-timeit"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.69.0"
  },
  "keywords": [
    "Python",
    "timeit",
    "execution time",
    "function"
  ],
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onCommand:vscode-python-timeit.timeIt"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-python-timeit.timeIt",
        "title": "Time It"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "preinstall": "npm install python-shell",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "python-shell": "^3.0.1"
  }
}
